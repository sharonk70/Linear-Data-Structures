   "source": [
    "def Pair(arr, n, sum):\n",
    "    count = 0  \n",
    "    for i in range(0, n):\n",
    "        for j in range(i + 1, n):\n",
    "            if arr[i] + arr[j] == sum:\n",
    "                count += 1\n",
    " \n",
    "    return count\n",
    " \n",
    " \n",
    "arr = [2, 10, 7, 8, 5]\n",
    "n = len(arr)\n",
    "sum = 8\n",
    "print(\"Count of pairs is\",\n",
    "      Pair(arr, n, sum))"
   ]
